{
  "testing": {
    "basicTestId": "test-form-field-input",
    "inputConfigCase": {
      "mainInput": {
        "type": "input",
        "config": {
          "inputConfig": {
            "type": "text"
          }
        }
      }
    },
    "selectConfigCase": {
      "mainInput": {
        "type": "select",
        "config": {
          "options": [
            { "id": "1", "name": "1" }
          ]
        }
      }
    },
    "checkboxConfigCase": {
      "mainInput": {
        "type": "checkbox",
        "config": {
          "content": "Is a form field checkbox"
        }
      }
    },
    "radioButtonConfigCase": {
      "mainInput": {
        "type": "radiobutton",
        "config": {
          "options": [
            {
              "label": "Is",
              "isChecked": false
            },
            {
              "label": "A",
              "isChecked": false
            },
            {
              "label": "Form",
              "isChecked": true
            },
            {
              "label": "Field",
              "isChecked": false
            }
          ],
          "name": "isRadioFormField"
        }
      }
    },
    "textAreaConfigCase": {
      "mainInput": {
        "type": "textarea",
        "config": {
          "cols": 3,
          "rows": 3,
          "name": "isRadioFormField"
        }
      }
    }
  },
  "storybook": {
    "parameters": {
      "componentSubtitle": "Generic form control component and designed for consistency",
      "docs": {
        "description": {
          "component": "Bulma's support of several native HTML form elements."
        }
      }
    }
  }
}